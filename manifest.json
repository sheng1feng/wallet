{
  "manifest_version": 3,
  "name": "YeyingMask",
  "version": "1.0",
  "description": "A Chrome extension like MetaMask.",
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "background": {
    "service_worker": "background/background.js"
  },
  "permissions": [
  ],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.google.com/*"],
      "js": ["content_scripts/content_script.js"],
      "css": ["content_scripts/content.styles.css"]
    }
  ],
  "options_page": "options/options.html"
}
